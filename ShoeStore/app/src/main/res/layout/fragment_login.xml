<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/linearcolorbackground"
        tools:context=".ui.LoginFragment">

        <ImageView
            android:layout_width="195dp"
            android:layout_height="188dp"
            android:src="@drawable/store"
            app:layout_constraintBottom_toTopOf="@+id/usernameInput"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.472"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.202" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="160dp"
            android:layout_height="68dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="79dp"
            android:fontFamily="@font/inter"
            android:lineSpacingExtra="1sp"
            android:text="@string/login_to_in"
            android:textAppearance="@style/login_to_in"
            android:textFontWeight="700"
            android:translationY="-0.66sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/username"
            android:layout_width="31dp"
            android:layout_height="19dp"
            android:layout_marginLeft="41dp"
            android:layout_marginTop="247dp"
            android:alpha="0.4"
            android:fontFamily="@font/inter"
            android:lineSpacingExtra="4sp"
            android:text="@string/username"
            android:textAppearance="@style/username"
            android:textFontWeight="400"
            android:translationY="-2.24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/usernameInput"
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:layout_marginTop="267dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/edittextstyle"
            android:elevation="6dp"
            android:ems="10"
            android:fontFamily="@font/inter"
            android:hint="@string/example_gmail_com"
            android:inputType="textEmailAddress"
            android:paddingLeft="21dp"
            android:textFontWeight="400"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.263"
            app:layout_constraintStart_toStartOf="@+id/username"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="56dp"
            android:layout_height="19dp"
            android:layout_marginLeft="41dp"
            android:layout_marginTop="337dp"
            android:alpha="0.4"
            android:fontFamily="@font/inter"
            android:lineSpacingExtra="4sp"
            android:text="@string/password"
            android:textAppearance="@style/password"
            android:textFontWeight="400"
            android:translationY="-2.24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/passwordInput"
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:layout_marginTop="357dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/edittextstyle"
            android:elevation="6dp"
            android:ems="10"
            android:fontFamily="@font/inter"
            android:hint="@string/passwordpoint"
            android:inputType="textPassword"
            android:paddingLeft="21dp"
            android:textFontWeight="400"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.263"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <View
            android:id="@+id/view3"
            android:layout_width="144dp"
            android:layout_height="1dp"
            android:layout_marginLeft="20.5dp"
            android:layout_marginTop="451.5dp"
            android:alpha="0.1"
            android:background="#171930"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.059"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="12dp"
            android:layout_height="19dp"
            android:layout_marginTop="442dp"
            android:alpha="1"
            android:fontFamily="@font/inter"
            android:lineSpacingExtra="4sp"
            android:text="@string/or"
            android:textAppearance="@style/or"
            android:textSize="12sp"
            android:translationY="-2.24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/view5"
            app:layout_constraintStart_toEndOf="@+id/view3"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.003" />

        <View
            android:id="@+id/view5"
            android:layout_width="144dp"
            android:layout_height="1dp"
            android:layout_marginLeft="211dp"
            android:layout_marginTop="451.5dp"
            android:alpha="0.1"
            android:background="#171930"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.243"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.003" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/facebookbutton"
            android:layout_width="99.67dp"
            android:layout_height="44dp"
            android:layout_marginTop="491dp"
            android:background="@drawable/buttonstyle"
            android:drawableStart="@drawable/facebook"
            android:paddingLeft="46dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.257"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/googlebutton"
            android:layout_width="99.67dp"
            android:layout_height="44dp"

            android:layout_marginTop="491dp"
            android:background="@drawable/buttonstyle"
            android:backgroundTint="#FF5252"
            android:drawableStart="@drawable/google"
            android:paddingLeft="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.723"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/forgotpassword"
            android:layout_width="107dp"
            android:layout_height="19dp"
            android:layout_marginTop="555dp"
            android:alpha="0.4"
            android:fontFamily="@font/inter"
            android:lineSpacingExtra="4sp"
            android:text="@string/forgot_password"
            android:textAppearance="@style/forgot_pass"
            android:textFontWeight="400"
            android:translationY="-2.24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.031" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/loginbutton"
            android:layout_width="335dp"
            android:layout_height="52dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="620dp"
            android:background="@drawable/linearbuttoncolor"
            android:fontFamily="@font/inter"
            android:text="@string/login"
            android:textColor="@color/white"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/forgotpassword"
            app:layout_constraintHorizontal_bias="0.548"
            app:layout_constraintStart_toStartOf="@+id/forgotpassword"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/registernewuser"
            android:layout_width="160dp"
            android:layout_height="15dp"
            android:alpha="0.4"
            android:fontFamily="@font/inter"
            android:lineSpacingExtra="0sp"
            android:text="@string/new_user_registration"
            android:textAppearance="@style/new_user"
            android:textFontWeight="400"
            android:translationY="-0.24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/loginbutton"
            app:layout_constraintStart_toStartOf="@+id/loginbutton"
            app:layout_constraintTop_toBottomOf="@+id/loginbutton"
            app:layout_constraintVertical_bias="0.34" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="20dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>